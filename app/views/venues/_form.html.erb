<%= simple_form_for(@venue) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :full_address %>
  </div>

  <div class="form-group">
      <%= f.label :region %>
      <% region = Region.all.map {|region| [region.name, region.id]} %>
      <%= f.select(:region_id, options_for_select(region)) %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
